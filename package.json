{
  "name": "marky-marked",
  "version": "4.0.1",
  "description": "A <textarea> so sweet you'll be feeling good vibrations.",
  "main": "dist/marky-marked.umd.js",
  "module": "dist/marky-marked.js",
  "format": "es6",
  "scripts": {
    "test": "concurrently npm:lint:* && karma start",
    "build": "concurrently npm:lint:* && concurrently npm:build:*",
    "lint:js:test": "standard './test/**/*.spec.js' | snazzy",
    "lint:js:src": "standard './src/**/*.js' | snazzy",
    "lint:css:src": "stylelint 'styles/**/*.css' -f string",
    "build:base": "rollup -c 'rollup.config.base.js'",
    "build:umd": "rollup -c 'rollup.config.umd.js'",
    "build:min": "rollup -c 'rollup.config.min.js'",
    "build:styles": "postcss 'styles/marky-marked.css' -c -o 'dist/marky-marked.css'",
    "tape": "browserify node_modules/tape/index.js --standalone test -o util/tape.js"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/patrickfatrick/marky-marked"
  },
  "keywords": [
    "Javascript",
    "textarea",
    "Markdown",
    "content editor",
    "WYSIWYG"
  ],
  "author": "Patrick Fricano <patrick.fricano@icloud.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "browserify": "^14.3.0",
    "codecov": "^3.1.0",
    "concurrently": "^4.1.0",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "faucet": "^0.0.1",
    "font-awesome": "^4.5.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-rollup-plugin": "^0.2.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-tap": "^4.1.4",
    "karma-tap-pretty-reporter": "^4.1.0",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-cli": "^3.1.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "sinon": "^2.1.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "contra": "^1.9.4",
    "harsh": "^1.4.0",
    "marked": "^0.3.5"
  }
}
